/* pages/personal_center/personal_center.wxss */
@import '../video_detail/video_detail';
@import '../index/index';

page {
  font-size: 24rpx;
}

.head {
  height: 200rpx;
  width: 92%;
  display: flex;
  align-items: center;
  padding: 0 4%;
  justify-content: space-between;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
}

.person {
  display: flex;
  align-items: center;
}

.portrait, .portrait1 {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  border: 1px solid white;
  /* box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.4); */
  margin-right: 40rpx;
  overflow: hidden;
}

.portrait image {
  width: 100%;
  height: 100%;
}

.next {
  width: 28rpx;
  height: 28rpx;
}

.person_select_item {
  padding: 0 4%;
  width: 92%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 88rpx;
  background-color: white;
  margin-bottom: 1px;
}

.person_select_item .image {
  width: 40rpx;
  height: 40rpx;
}

.person_select_item .person .image {
  margin-right: 20rpx;
}

.flex_center {
  display: flex;
  align-items: center;
}

.per_con image {
  width: 30rpx;
  height: 30rpx;
}

.colorfff {
  color: #fff;
}

.per_con>view:nth-of-type(2) {
  margin: 10rpx 0;
}

.per_con>view>.flex_center {
  margin-right: 30rpx;
}

.detail_btn {
  width: 100%;
  margin: 44rpx 0 54rpx 0;
}

.detail_btn>view {
  width: 50%;
  margin: 0;
}

.xian {
  display: inline-block;
  height: 60rpx;
  width: 2rpx;
  background-color: #ccc;
}

.username {
  font-size: 30rpx;
}

.mobile {
  font-size: 24rpx;
}

.image_play_num {
  margin-right: 12rpx;
  width: 30rpx;
  height: 30rpx;
}

.image_click_num {
  margin-right: 12rpx;
  width: 30rpx;
  height: 30rpx;
}

.collect_container {
  display: flex;
  flex-direction: column;
  justify-items: center;
}

.collect-row-center {
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.collect_circle {
  width: 200rpx;
  height: 200rpx;
  background-color: #e6e6e6;
  border-radius: 50%;
  margin-top: 70rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.collect_circle image {
  width: 98rpx;
  height: 84rpx;
  padding-top: 58rpx;
  padding-bottom: 58rpx;
}

.collect_circle_text {
  text-align: center;
  width: 330rpx;
  height: 28rpx;
  color: #ccc;
  font-size: 28rpx;
  margin-top: 36rpx;
}

.label>view>.check_no_text {
  color: #DD0202;
}

.label>view>.check_success_text {
  color: #04BC23;
}

.label>view>.checking_text {
  color: #0D16D7;
}

.check_container {
  margin-top: 12rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.check_container image {
  width: 30rpx;
  height: 30rpx;
}

.check_notice_box {
  border-radius: 10rpx;
  border: 1px solid #f2f2f2;
  margin-right: 34rpx;
  background-color: #f2f2f2;
  padding-left: 12rpx;
  padding-right: 12rpx;
}
.label .check_no_text{
  color: #DD0202;
}